{
    "manifest_version": 2,
  
    "name": "Budget Manager",
    "description": "This extension tracks your overall spendings.",
    "version": "1.0",
    "icons": {
      "128": "bank128.png",
      "48": "bank48.png",
      "16": "bank16.png"
    },
  
    "browser_action": {
      "default_icon": "bank16.png",
      "default_popup": "popup.html"
    },
  
    "options_page": "options.html",

    "permissions": [
            "storage",
            "notifications"
          ]
  
    
  }